<?xml version='1.0' encoding='utf-8'?>
<shallow_water_options>
  <simulation_name>
    <string_value lines="1">dvs_channel</string_value>
  </simulation_name>
  <geometry>
    <dimension>
      <integer_value rank="0">2</integer_value>
    </dimension>
    <mesh>
      <from_file>
        <relative_path>
          <string_value lines="1">src/dvs_channel.msh</string_value>
        </relative_path>
      </from_file>
    </mesh>
  </geometry>
  <function_spaces>
    <function_space name="VelocityFunctionSpace">
      <degree>
        <integer_value rank="0">2</integer_value>
      </degree>
      <family>
        <string_value>Continuous Lagrange</string_value>
      </family>
    </function_space>
    <function_space name="FreeSurfaceFunctionSpace">
      <degree>
        <integer_value rank="0">1</integer_value>
      </degree>
      <family>
        <string_value>Continuous Lagrange</string_value>
      </family>
    </function_space>
  </function_spaces>
  <io>
    <dump_format>
      <string_value>vtk</string_value>
    </dump_format>
    <dump_period>
      <constant>
        <real_value rank="0">0</real_value>
      </constant>
    </dump_period>
  </io>
  <timestepping>
    <current_time>
      <real_value rank="0">0</real_value>
    </current_time>
    <timestep>
      <real_value rank="0">5.0</real_value>
    </timestep>
    <nonlinear_iterations>
      <integer_value rank="0">2</integer_value>
      <tolerance>
        <real_value rank="0">1.0e-7</real_value>
      </tolerance>
    </nonlinear_iterations>
    <finish_time>
      <real_value rank="0">10000.0</real_value>
    </finish_time>
  </timestepping>
  <physical_parameters>
    <gravity>
      <magnitude>
        <real_value rank="0">9.81</real_value>
      </magnitude>
    </gravity>
  </physical_parameters>
  <system>
    <core_fields>
      <vector_field name="Velocity">
        <initial_condition>
          <constant>
            <real_value shape="2" dim1="dim" rank="1">1e-16 1e-16</real_value>
          </constant>
        </initial_condition>
        <boundary_condition name="Right_End">
          <surface_ids>
            <integer_value shape="1" rank="1">4</integer_value>
          </surface_ids>
          <type name="flather">
            <exterior_velocity>
              <cpp>
                <string_value lines="20" type="code" language="python">def val(t):
  period = 1.24 * 60 * 60 # 1.24 hours in seconds
  expr = "2.0*sin(pi*t/%s)" % str(period/2.0)
  return [expr, "0.0"]</string_value>
              </cpp>
            </exterior_velocity>
            <exterior_free_surface_perturbation>
              <cpp>
                <string_value lines="20" type="code" language="python">def val(t):
  period = 1.24 * 60 * 60 # 1.24 hours in seconds
  expr = "-cos(pi*t/%s)" % str(period/2.0)
  return expr</string_value>
              </cpp>
            </exterior_free_surface_perturbation>
          </type>
        </boundary_condition>
        <boundary_condition name="Left_End">
          <surface_ids>
            <integer_value shape="1" rank="1">3</integer_value>
          </surface_ids>
          <type name="flather">
            <exterior_velocity>
              <cpp>
                <string_value lines="20" type="code" language="python">def val(t):
  period = 1.24 * 60 * 60 # 1.24 hours in seconds
  expr = "2.0*sin(pi*t/%s)" % str(period/2.0)
  return [expr, "0.0"]</string_value>
              </cpp>
            </exterior_velocity>
            <exterior_free_surface_perturbation>
              <cpp>
                <string_value lines="20" type="code" language="python">def val(t):
  period = 1.24 * 60 * 60 # 1.24 hours in seconds
  expr = "cos(pi*t/%s)" % str(period/2.0)
  return expr</string_value>
              </cpp>
            </exterior_free_surface_perturbation>
          </type>
        </boundary_condition>
        <boundary_condition name="Sides">
          <surface_ids>
            <integer_value shape="2" rank="1">5 6</integer_value>
          </surface_ids>
          <type name="no_normal_flow"/>
        </boundary_condition>
      </vector_field>
      <scalar_field name="FreeSurfacePerturbation">
        <initial_condition>
          <cpp>
            <string_value lines="20" type="code" language="python">def val(t):
  expr = "cos(pi/3e3*x[0])"
  return expr</string_value>
          </cpp>
        </initial_condition>
      </scalar_field>
      <scalar_field name="FreeSurfaceMean">
        <value>
          <constant>
            <real_value rank="0">50.0</real_value>
          </constant>
        </value>
      </scalar_field>
    </core_fields>
    <equations>
      <continuity_equation>
        <spatial_discretisation>
          <continuous_galerkin/>
        </spatial_discretisation>
        <integrate_by_parts/>
      </continuity_equation>
      <momentum_equation>
        <spatial_discretisation>
          <continuous_galerkin>
            <stabilisation>
              <streamline_upwind_stabilisation/>
            </stabilisation>
          </continuous_galerkin>
        </spatial_discretisation>
        <mass_term/>
        <advection_term/>
        <stress_term>
          <scalar_field name="Viscosity">
            <value>
              <constant>
                <real_value rank="0">1.0</real_value>
              </constant>
            </value>
          </scalar_field>
        </stress_term>
        <drag_term>
          <scalar_field name="DragCoefficient">
            <value>
              <cpp>
                <string_value lines="20" type="code" language="python">def val(t):
  C_D = "x[0] &gt;= 1500 &amp;&amp; x[0] &lt;= 1520 &amp;&amp; x[1] &gt;= 480 &amp;&amp; x[1] &lt;= 520 ? 12 : 0.0025"
  #C_D = 0.0025
  return C_D</string_value>
              </cpp>
            </value>
          </scalar_field>
        </drag_term>
      </momentum_equation>
    </equations>
  </system>
</shallow_water_options>
